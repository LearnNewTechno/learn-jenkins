pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    sh "this is build"  
                }       
            }
        }
        stage('Test') {
            steps {
                script {
                    sh "this is Test"  
                }  
            }
        }
        stage('Deploy') {
            steps {
                script {
                    sh "this is Deploy"  
                }  
            }
        }
    }
}